-- ‚úÖ CONFIGURA√á√ÉO DO TELEPORTE
local placeId = 17503543197
local jobId = "143c1961-a718-44e8-ba2d-a7d08b64fe33"

-- ‚úÖ CONFIGURA√á√ÉO GLOBAL PARA CONTROLAR O TELEPORTE
getgenv().UseCustomJobTeleport = false  -- ‚úÖ true = ativa o teleporte / false = ignora o teleporte

-- üöÄ TELEPORTE AUTOM√ÅTICO (se ativado e necess√°rio)
task.spawn(function()
    local TeleportService = game:GetService("TeleportService")
    local currentJobId = game.JobId

    if getgenv().UseCustomJobTeleport and currentJobId ~= jobId then
        local success, err = pcall(function()
            TeleportService:TeleportToPlaceInstance(placeId, jobId, game.Players.LocalPlayer)
        end)
        if not success then
            warn("Falha ao teleportar:", err)
        end
    else
        warn("Teleporte desativado ou j√° est√° no servidor alvo.")
    end

    -- ‚è≥ DELAY PARA CARREGAR O SCRIPT DO EVENTO
    task.wait(10)
    pcall(function()
        getgenv().AutoSummerEvent = {
            ["_GEnabled"] = true,
            ["_GWebhookID"] = "426889632488095754",
            ["_GWebhookURL"] = "https://discord.com/api/webhooks/1257767230242820116/tVrRuIGGCfMYMZQIxMvL8n9AATDYIWtfzeomCRIwnWLtyiUBmxwYAe9aj3NcmelUfZNW",
            ["_GOptimizationsLastestVersion"] = true,
            ["_GOptimizationsStatUI"] = true,
            ["_GOptimizationAutoRejoin"] = nil,
            ["_GAutoServerHop"] = true,
            ["_GAutoServerHopEggChance"] = {"100x"},
            ["_GAutoTiles"] = true,
            ["_GTilesDelay"] = 1,
            ["_GAutoRewards"] = true,
            ["_GRewardsDelay"] = 20,
            ["_GAutoRaffle"] = true,
            ["_GRaffleAmount"] = 10,
            ["_GHatchStopIslandProgress"] = 95,
            ["_GHatchEggs"] = true,
            ["_GHatchDelay"] = 0.2,
            ["_GAutoRebirth"] = false,
            ["_GRebirthDelay"] = 120,
            ["_GSkipRebirthOnNormalEgg"] = {"100x"},
            ["_GSkipRebirthOnTitanicEgg"] = {"100x"},
            ["_GSkipRebirthOnSelectedTitanicEggs"] = {"Five", "Six", "Seven", "Eight"},
            ["_GEggScanInterval"] = 1,
            ["_GAutoTeleportToOthersEggs"] = true,
            ["_GAutoTeleportToBestEgg"] = true,
            ["_GAutoTeleportToBestTitanicEgg"] = true,
            ["_GAutoTeleportToBestSelectedTitanicEggs"] = {"Five", "Six", "Seven", "Eight"},
        }

        loadstring(game:HttpGet("https://raw.githubusercontent.com/Sub2BK/Stratum/refs/heads/Scripts/PS99_Event.lua"))()
    end)
end)

-- üõ°Ô∏è ANTI-LOG
task.spawn(function()
    task.wait(4)
    for _, v in pairs(getconnections(game.LogService.MessageOut)) do
        pcall(function()
            v:Disconnect()
        end)
    end
end)
